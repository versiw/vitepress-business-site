{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "outDir": ".vitepress/dist",
    "declaration": true,
    "declarationDir": "./dist",
    "emitDeclarationOnly": true,
    "types": ["vite/client"]
  },
  "include": ["./src/**/*.ts", "./.vitepress/**/*.ts", "./.vitepress/**/*.vue"],
  "exclude": ["node_modules"]
}
